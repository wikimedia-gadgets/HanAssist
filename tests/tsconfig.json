{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM",
    ],
    "noEmit": true,
    "strict": true,
    "rootDir": ".",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "references": [
    {
      "path": ".."
    }
  ],
  "include": [
    "**/*.ts"
  ],
  "exclude": []
}
